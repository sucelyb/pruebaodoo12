<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="digifact_view_move_form" model="ir.ui.view">
            <field name="name">digifactfel.account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <page name="other_info" position="after">
                    <page name="feel" string="FEEL">
                        <group>
                            <group>
                                <field name="acuse_recibo_sat"/>
                                <field name="codigo_sat"/>
                                <field name="feel_numero_autorizacion"/>
                                <field name="feel_serie"/>
                                <field name="feel_numero"/>
                                <!-- <field name="feel_uuid"/>
                                <field name="feel_documento_certificado" widget ="url"/> -->
                            </group>
                            <group>
                                <field name="formato_xml"/>
                                <field name="formato_html"/>
                                <field name="formato_pdf"/>
                            </group>
                        </group>
                    </page>
                </page>
            </field>
        </record>

    </data>
</odoo>
