<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_feel_infile_account_journal_form" model="ir.ui.view">
            <field name="name">feel_infile.account.journal.form</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <page name="journal_entries" position="after">
                    <page name="feel_infile" string="FEEL">
                        <group>
                            <group>
                                <field name="feel_tipo_dte"/>
                                <field name="feel_codigo_establecimiento"/>
                                <field name="feel_usuario"/>
                                <field name="feel_llave_pre_firma"/>
                                <field name="feel_llave_firma"/>
                                <field name="feel_nombre_comercial"/>
                            </group>
                            <group>
                                <field name="producto_descripcion"/>
                                <field name="descripcion_factura"/>
                            </group>
                        </group>
                    </page>
                </page>
            </field>
        </record>


    </data>
</odoo>
